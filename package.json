{
  "name": "@nuwcdivnpt/stigman-watcher",
  "version": "1.5.4",
  "description": "CLI that watches a path for STIG test result files on behalf of a STIG Manager Collection.",
  "main": "index.js",
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/NUWCDIVNPT/stigman-watcher.git"
  },
  "bin": {
    "stigman-watcher": "index.js"
  },
  "scripts": {
    "test": "c8 --reporter=html --reporter=text mocha  './test/**/*.test.js'",
    "test:watch": "nodemon --ext 'js' --exec 'npm test'"
  },
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "@nuwcdivnpt/stig-manager-client-modules": "^1.5.6",
    "atob": "^2.1.2",
    "better-queue": "^3.8.12",
    "chokidar": "^4.0.3",
    "commander": "^14.0.0",
    "dotenv": "^17.2.2",
    "fast-glob": "3.3.3",
    "got": "^14.4.8",
    "jsonwebtoken": "^9.0.2",
    "n-readlines": "^1.0.1",
    "prompt-sync": "4.2.0",
    "semver": "^7.7.2",
    "serialize-error": "^12.0.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "c8": "^10.1.3",
    "chai": "^6.0.1",
    "esbuild": "^0.25.9",
    "mocha": "^11.7.2",
    "nodemon": "^3.1.10"
  }
}
